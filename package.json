    }
  ],
  "keywords": [
    "homebridge",
    "homebridge-plugin",
    "homekit",
    "adt",
    "adt-pulse",
    "icontrol",
    "security",
    "security-system"
  ],
  "author": {
    "name": "Jacky Liang",
