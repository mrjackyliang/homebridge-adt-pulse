    "globals": "15.14.0",
    "homebridge": "1.8.5",
    "npm-run-all": "4.1.5",
    "rimraf": "6.0.1",
    "tsconfig-replace-paths": "0.0.14",
    "typescript": "5.7.2",
    "vite": "7.1.10",
    "vite-tsconfig-paths": "5.1.4"
  }
